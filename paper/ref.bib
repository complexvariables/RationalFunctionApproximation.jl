@article{doi:10.1137/20M1322698,
  title = {The P-{{AAA}} Algorithm for Data-Driven Modeling of Parametric Dynamical Systems},
  author = {Rodriguez, Andrea Carracedo and Balicki, Linus and Gugercin, Serkan},
  year = {2023},
  journal = {SIAM Journal on Scientific Computing},
  volume = {45},
  number = {3},
  eprint = {https://doi.org/10.1137/20M1322698},
  pages = {A1332-A1358},
  doi = {10.1137/20M1322698},
  abstract = {Abstract. The AAA algorithm has become a popular tool for data-driven rational approximation of single-variable functions, such as transfer functions of a linear dynamical system. In the setting of parametric dynamical systems appearing in many prominent applications, the underlying (transfer) function to be modeled is a multivariate function. With this in mind, we develop the AAA framework for approximating multivariate functions where the approximant is constructed in the multivariate barycentric form. The method is data driven, in the sense that it does not require access to the full state-space model and requires only function evaluations. We discuss an extension to the case of matrix-valued functions, i.e., multi-input/multi-output dynamical systems, and provide a connection to the tangential interpolation theory. Several numerical examples illustrate the effectiveness of the proposed approach.}
}

@article{driscollAAARational2024,
