@article{rodriguezPAAAAlgorithm2023,
  title = {The p-{{AAA}} Algorithm for Data-Driven Modeling of Parametric Dynamical Systems},
  author = {Rodriguez, A. C. and Balicki, L. and Gugercin, S.},
  year = {2023},
  month = jun,
